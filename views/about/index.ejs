    <script>
        ncebCardsGlider = new Glider(document.querySelector('#ncebCards'), {
            slidesToShow: 3,
            slidesToScroll: 1,
            scrollLock: true,
            draggable: true,
            duration: 2,
            responsive: [{
                breakpoint: 480,
                settings: {
                    slidesToShow: 2,
                    slidesToScroll: 'auto'
                }
            }, {
                breakpoint: 600,
                settings: {
                    slidesToShow: 3,
                    slidesToScroll: 'auto'
                }
            }, {
                breakpoint: 1024,
                settings: {
                    slidesToShow: 6,
                    slidesToScroll: 'auto'
                }
            }]
        });

        function sliderAuto(slider, miliseconds) {
            const slidesCount = slider.track.childElementCount;
            let slideTimeout = null;
            let nextIndex = 1;

            function slide() {
                slideTimeout = setTimeout(
                    function() {
                        if (nextIndex >= slidesCount) {
                            nextIndex = 0;
                        }
                        slider.scrollItem(nextIndex++);
                    },
                    miliseconds
                );
            }

            slider.ele.addEventListener('glider-animated', function() {
                window.clearInterval(slideTimeout);
                slide();
            });

            slide();
        }

        sliderAuto(ncebCardsGlider, 2000);
    </script>