<script>
    document.addEventListener('DOMContentLoaded', function() {
        const form = document.getElementById('certVerificationForm');
        const input = document.getElementById('certInput');
        const button = document.getElementById('crv-button');
        
        form.addEventListener('submit', function(e) {
            if (!input.value.trim()) {
                e.preventDefault();
                alert('Please enter a certificate number or verification code');
                return false;
            }
            
            // Show loading state
            const originalText = button.innerHTML;
            button.disabled = true;
            button.innerHTML = '<span class="spinner-border spinner-border-sm me-2"></span>Verifying...';
            
            // Re-enable after a delay (form will submit)
            setTimeout(() => {
                button.disabled = false;
                button.innerHTML = originalText;
            }, 2000);
        });
        
        // Auto-uppercase input
        input.addEventListener('input', function() {
            this.value = this.value.toUpperCase();
        });
    });
    </script>